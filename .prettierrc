{
    "overrides": [
        {
            "files": "*",
            "excludeFiles": ["dis/*", "styling/dist/*", "node_modules/*", "www/*"],
            "options": {
                "arrowParens": "avoid",
                "bracketSpacing": true,
                "htmlWhitespaceSensitivity": "css",
                "insertPragma": false,
                "jsxBracketSameLine": false,
                "jsxSingleQuote": true,
                "printWidth": 90,
                "proseWrap": "always",
                "quoteProps": "as-needed",
                "requirePragma": false,
                "semi": true,
                "singleQuote": true,
                "tabWidth": 4,
                "trailingComma": "all",
                "useTabs": false,
                "vueIndentScriptAndStyle": false
            }
        },
        {
            "files": ".prettierrc",
            "options": {
                "parser": "json"
            }
        },
        {
            "files": "*.json",
            "options": {
                "parser": "json"
            }
        }
    ]
}
